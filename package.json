{
  "name": "colorado-rental-assistant-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server:dev": "NODE_ENV=development tsx --tsconfig tsconfig.server.json server/index.ts",
    "server:build": "echo 'Building server...' && tsc -p tsconfig.server.json && echo 'Server build complete'",
    "server:start": "echo 'Starting server...' && NODE_ENV=production node dist/server/index.js",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "npm run server:start"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
