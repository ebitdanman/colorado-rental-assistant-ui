    "@chakra-ui/theme": "^3.4.6",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "framer-motion": "^6.5.1",
    "openai": "^4.89.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
